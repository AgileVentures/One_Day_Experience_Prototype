<column class="col-sacrificial1 span_1_of_16"></column>
<h1>Profiles search</h1>
  <%= form_for search_path, method: :get do |f| %>
  <p>
  <%= f.label "search_for"%>
  <%= text_field_tag :q, params[:q] %>
  <%= submit_tag "Go", name: nil %>
  </p>
  <% end %>

<grid>
  <div class="container">
    <% @profiles.each do |profile| %>
      <section class="new-card-form span_4_of_16">
        <h5> <%= profile.name %> </h5>
        <h5> <%= profile.company %> </h5>
        <h5> <%= profile.info %> </h5>
      </section>
    <% end %>
  </div>
</grid>